<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
	<select id="selectUserByEmail" parameterType="hashmap" resultType="hashmap">
		SELECT *
		  FROM CODING_MEMBERS
		WHERE EMAIL = #{email}
	</select>

</mapper>